<Window x:Class="NF.Tool.UnityPackage.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NF.Tool.UnityPackage.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <TabControl Name="tcSample">
            <TabItem x:Name="Packer" Header="Packer" Selector.Selected="OnTabSelected" >
            </TabItem>
            <TabItem x:Name="Unpacker" Header="Unpacker" Selector.Selected="OnTabSelected">
            </TabItem>
            <TabItem x:Name="About" Header="About" Selector.Selected="OnTabSelected">
            </TabItem>
        </TabControl>
    </DockPanel>
</Window>
